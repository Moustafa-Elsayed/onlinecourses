{
    "version": 2,
    "builds": [
      {
        "src": "package.json",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "/api/index.js"
      },
      {
        "src": "/(.*)",
        "dest": "/public/$1"
      }
    ],
    "env": {
      "API_URL": "https://api.example.com",
      "ANOTHER_ENV_VAR": "value"
    },
    "functions": {
      "api/**/*.js": {
        "maxDuration": 10
      }
    }
  }
  